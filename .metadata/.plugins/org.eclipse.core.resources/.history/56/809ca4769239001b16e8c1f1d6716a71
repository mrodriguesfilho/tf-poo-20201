package banco;

import java.util.ArrayList;

public abstract class Banco {

		public abstract boolean criarConta(Conta dadosConta);
		public abstract Conta login(Conta dadosLogin);
		public abstract boolean deposito(Conta conta, double valor);
		public abstract boolean saque(Conta conta, double valor);
		public abstract boolean transferir(Conta conta1, Conta conta2, double valor);
		public abstract double saldo(Conta conta);
		public abstract ArrayList<String> extrato(Conta conta);
		
}

